  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev-debug",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "${workspaceFolder}/lib/main_dev.dart",
      "args": [
        "--flavor",
        "dev",
        "-t",
        "lib/main_dev.dart"
      ]
    },
    {
      "name": "dev-profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "${workspaceFolder}/lib/main_dev.dart",
      "args": [
        "--flavor",
        "dev",
        "-t",
        "lib/main_dev.dart"
      ]
    },
    {
      "name": "prod-debug",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "${workspaceFolder}/lib/main_prod.dart",
      "args": [
        "--flavor",
        "prod",
        "-t",
        "lib/main_prod.dart"
      ]
    },
    {
      "name": "prod-profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "${workspaceFolder}/lib/main_prod.dart",
      "args": [
        "--flavor",
        "prod",
        "-t",
        "lib/main_prod.dart"
      ]
    },
    {
      "name": "prod-release",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "${workspaceFolder}/lib/main_prod.dart",
      "args": [
        "--flavor",
        "prod",
        "-t",
        "lib/main_prod.dart"
      ]
    },
  ]
